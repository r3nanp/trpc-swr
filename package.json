{
	"name": "trpc-swr-mono",
	"version": "1.0.0",
	"description": "Monorepo of trpc-swr",
	"dependencies": {
		"turbo": "^1.7.0"
	},
	"devDependencies": {
		"husky": "8.0.3",
		"nano-staged": "0.8.0"
	},
	"nano-staged": {
		"*.{js,ts,md,json,yaml}": "dprint fmt"
