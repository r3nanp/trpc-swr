{
  "name": "trpc-swr",
  "version": "1.0.0-rc.6",
  "private": false,
  "description": "tRPC-ified SWR hooks",
  "repository": {
    "type": "git",
    "url": "https://github.com/sachinraja/trpc-swr.git"
  },
  "homepage": "https://github.com/sachinraja/trpc-swr#readme",
  "bugs": {
    "url": "https://github.com/sachinraja/trpc-swr/issues"
  },
  "keywords": [
    "trpc",
    "swr",
    "react",
    "hooks",
    "typescript",
    "api",
    "rpc",
    "rest",
    "http",
    "fetch",
    "query",
    "mutation"
  ],
  "author": "Sachin Raja <sachinraja2349@gmail.com>, Sanna Jammeh <hello@sannajammeh.com>",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "module": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./infinite": {
      "module": "./dist/infinite/index.js",
      "import": "./dist/infinite/index.js",
      "require": "./dist/infinite/index.cjs",
      "types": "./dist/infinite/index.d.ts"
    },
    "./next": {
      "module": "./dist/next/index.js",
      "import": "./dist/next/index.js",
      "require": "./dist/next/index.cjs",
      "types": "./dist/next/index.d.ts"
    },
    "./ssg": {
      "module": "./dist/ssg/index.js",
      "import": "./dist/ssg/index.js",
      "require": "./dist/ssg/index.cjs",
      "types": "./dist/ssg/index.d.ts"
    }
  },
  "files": [
    "./dist/**/*.{js,d.ts,cjs,mjs}",
    "./package.json",
    "./infinite",
    "./next",
    "./ssg",
    "./README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "bunchee -w",
    "format": "dprint fmt",
    "lint:format": "dprint check",
    "lint:types": "tsc",
    "lint:js": "eslint .",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "@trpc/client": ">=10.11.1",
    "@trpc/server": ">=10.11.1",
    "react": ">=17 || >=18",
    "swr": ">=2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@sachinraja/eslint-config": "0.1.1",
    "@testing-library/dom": "9.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@trpc/client": "10.14.0",
    "@trpc/server": "10.14.0",
    "@types/node": "18.14.5",
    "@types/react": "18.0.28",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "@vitest/ui": "^0.29.2",
    "bunchee": "2.2.0",
    "c8": "^7.13.0",
    "eslint": "8.35.0",
    "get-port": "6.1.2",
    "jsdom": "21.1.0",
    "node-fetch": "^3.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "^4.2.0",
    "swr": "2.0.4",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "undici": "5.20.0",
    "vite": "4.1.4",
    "vitest": "0.29.2",
    "zod": "3.20.6"
  },
  "sideEffects": false,
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/infinite/index.ts",
      "src/next/index.ts",
      "src/ssg/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true
  }
}
