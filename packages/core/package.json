{
	"name": "trpc-swr",
	"version": "1.0.0-rc.1",
	"private": false,
	"description": "tRPC-ified SWR hooks",
	"repository": {
		"type": "git",
		"url": "https://github.com/sachinraja/trpc-swr.git"
	},
	"homepage": "https://github.com/sachinraja/trpc-swr#readme",
	"bugs": {
		"url": "https://github.com/sachinraja/trpc-swr/issues"
	},
	"author": "Sachin Raja <sachinraja2349@gmail.com>, Sanna Jammeh <hello@sannajammeh.com>",
	"license": "MIT",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		},
		"./infinite": {
			"import": "./dist/infinite/index.js",
			"require": "./dist/infinite/index.cjs",
			"types": "./dist/infinite.d.ts"
		},
		"./next": {
			"import": "./dist/next/index.js",
			"require": "./dist/next/index.cjs",
			"types": "./dist/next.d.ts"
		},
		"./ssg": {
			"import": "./dist/ssg/index.js",
			"require": "./dist/ssg/index.cjs",
			"types": "./dist/ssg.d.ts"
		}
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*.{js,d.ts,cjs,mjs}"
	],
	"scripts": {
		"build": "bunchee",
		"dev": "bunchee -w",
		"format": "dprint fmt",
		"lint": "run-p lint:*",
		"lint:format": "dprint check",
		"lint:types": "tsc",
		"lint:js": "eslint .",
		"prepublishOnly": "pnpm build",
		"test": "vitest run",
		"test:ui": "vitest --ui"
	},
	"peerDependencies": {
		"@trpc/client": ">=10.5.0",
		"@trpc/server": ">=10.5.0",
		"react": ">=17 || >=18",
		"swr": ">=2.0.1"
	},
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@sachinraja/eslint-config": "0.1.1",
		"@testing-library/dom": "8.20.0",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/react": "13.4.0",
		"@testing-library/user-event": "14.4.3",
		"@trpc/client": "10.9.0",
		"@trpc/server": "10.9.0",
		"@types/node": "18.11.18",
		"@types/react": "18.0.27",
		"@vitejs/plugin-react-swc": "^3.0.1",
		"@vitest/ui": "^0.27.2",
		"bunchee": "^2.2.0",
		"c8": "^7.12.0",
		"cross-fetch": "^3.1.5",
		"dprint": "0.34.4",
		"eslint": "8.32.0",
		"get-port": "6.1.2",
		"jsdom": "21.0.0",
		"npm-run-all": "4.1.5",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"swr": "2.0.1",
		"tsup": "6.5.0",
		"typescript": "^4.9.4",
		"undici": "5.15.1",
		"vite": "4.0.4",
		"vitest": "0.27.2",
		"zod": "3.20.2"
	},
	"sideEffects": false,
	"tsup": {
		"entry": [
			"src/index.ts",
			"src/infinite/index.ts",
			"src/next/index.ts",
			"src/ssg/index.ts"
		],
		"format": [
			"esm",
			"cjs"
		],
		"dts": {
			"resolve": true
		},
		"splitting": true,
		"clean": true
	}
}
